{
  "version": 2,
  "functions": {
    "_middleware": {
      "runtime": "edge"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://db.goxi.top/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}